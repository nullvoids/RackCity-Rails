<!DOCTYPE html>
<html>
<head>
  <title>RackCity</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>

  <% if notice %>
  <p class='notice'>
    <%= notice %>
  </p>
  <% end %>
  <% if alert %>
  <p class='alert'>
    <%= alert %>
  </p>
  <% end %>
</head>
<body>

  <%= yield %>

</body>
</html>
